type Tweet = 
 record {
   handle: text;
   id: nat;
   likes: nat;
   retweets: nat;
   "text": text;
   timestamp: int;
   username: text;
 };
service : {
  createTweet: (text, text, text) -> (Tweet);
  getTweets: () -> (vec Tweet) query;
  likeTweet: (nat) -> (bool);
  retweetTweet: (nat) -> (bool);
}
